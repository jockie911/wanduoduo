<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/bgcolor_deep">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_45_dip"
        android:layout_marginLeft="@dimen/dimen_21_dip"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/item_iv_isChecked"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/index_is_checked"/>
        <TextView
            android:id="@+id/tv_sex_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c_208_210_211"
            android:textSize="15sp"
            android:text="不限男女"
            android:layout_toRightOf="@+id/icon_temp1"
            android:layout_marginLeft="@dimen/dimen_10_dip" />
    </LinearLayout>

</FrameLayout>